{
  "manifest_version": 2,
  "name": "New Capital Exchange cryptocurrency values",
  "description":
    "Extension to show cryptocurrency values from New Capital Exchange",
  "version": "1",
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
   "128": "assets/icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "New Capital Exchange cryptocurrency values"
  },
  "permissions": [
    "https://api.new.capital/"
  ]
}
